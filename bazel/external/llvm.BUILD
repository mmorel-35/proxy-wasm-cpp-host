# Platform-specific config settings to enable proper select() in alias rule
config_setting(
    name = "linux_x86_64",
    constraint_values = [
        "@platforms//os:linux",
        "@platforms//cpu:x86_64",
    ],
)

config_setting(
    name = "linux_aarch64",
    constraint_values = [
        "@platforms//os:linux",
        "@platforms//cpu:aarch64",
    ],
)

config_setting(
    name = "macos_x86_64",
    constraint_values = [
        "@platforms//os:macos",
        "@platforms//cpu:x86_64",
    ],
)

config_setting(
    name = "macos_aarch64",
    constraint_values = [
        "@platforms//os:macos",
        "@platforms//cpu:aarch64",
    ],
)

config_setting(
    name = "windows_x86_64",
    constraint_values = [
        "@platforms//os:windows",
        "@platforms//cpu:x86_64",
    ],
)

# Platform and architecture-aware alias that selects the appropriate LLVM configuration
alias(
    name = "llvm_lib",
    actual = select({
        ":linux_x86_64": ":llvm_lib_linux_x86",
        ":linux_aarch64": ":llvm_lib_linux_aarch64",
        ":macos_x86_64": ":llvm_lib_macos_x86",
        ":macos_aarch64": ":llvm_lib_macos_aarch64",
        ":windows_x86_64": ":llvm_lib_windows_x86",
        # Default to x86_64 Linux for other architectures
        "//conditions:default": ":llvm_lib_linux_x86",
    }),
)
